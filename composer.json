{
    "name": "sourcefli/permission-name",
    "description": "Create and Retrieve permissions using conventions",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonathan Havens",
            "email": "mail@jhavens.tech",
            "homepage": "https://jhavens.tech",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Sourcefli\\PermissionName\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sourcefli\\PermissionName\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require": {
        "php": "^7.4|^8.0",
        "spatie/once": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sourcefli\\PermissionName\\PermissionNameServiceProvider"
            ],
            "aliases": {
                "OwnedPermission": "Sourcefli\\PermissionName\\Facades\\OwnedPermission"
            }
        }
    }
}
